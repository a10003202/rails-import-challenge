<div>
  <!--breadcrumb-->
  <div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
    <div class="breadcrumb-title pr-3"><%= t('admin.users.breadcrumb') %></div>
    <div class="pl-3">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb mb-0 p-0">
          <li class="breadcrumb-item"><a href="javascript:void(0);"><i class='bx bx-home-alt'></i></a>
          </li>
          <li class="breadcrumb-item active" aria-current="page"><%= t('label.table') %></li>
        </ol>
      </nav>
    </div>
    <div class="ml-auto">
      <a href="javascript:void(0);" ng-click="create()" class="btn btn-primary"><%= t('button.create') %></a>
      <%= link_to 'New Admin User', new_admin_user_path %>
    </div>
  </div>
  <!--end breadcrumb-->
  <div class="card">
    <div class="card-body">
      <div class="card-title">
        <h1 class="mb-0"><%= t('.title') %></h1>
      </div>
      <hr/>
      <div class="table-responsive" ng-init="loadDatatable()">
        <table class="responsive nowrap table table-hover" id="dttb-admin_users" data-sort="true" data-source="<%= url_for(format: :json)%>">
          <thead>
          <tr>
            <th data-data="id" data-orderable="true" data-class-name="all" data-searchable="true" class="skip-export" width="100px">Id</th>
            <th data-data="name" data-orderable="true" data-class-name="all" data-searchable="true" class="skip-export" width="100px">Nombre</th>
          </tr>
          </thead>

        </table>
      </div>
    </div>
  </div>
</div>